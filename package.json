{
  "name": "fp-booleans",
  "version": "0.2.0",
  "description": "Utilities to apply boolean logic to functions and higher-order functions. Tiny, tested and composable",
  "author": "Pierluigi Pesenti",
  "license": "MIT",
  "keywords": [
    "boolean",
    "logic",
    "functional-programming",
    "functional programming",
    "fp",
    "higher-order-functions",
    "composition",
    "utility",
    "javascript",
    "typescript",
    "composable",
    "operators",
    "logical-operations",
    "utility-functions",
    "development-tools",
    "programming",
    "code-simplification",
    "not",
    "and",
    "or",
    "predicate-functions",
    "functional-utilities"
  ],
  "type": "module",
  "repository": "https://github.com/Oaxoa/fp-booleans",
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier '**/*.ts'",
    "prettier:fix": "prettier --write '**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build:ts": "tsc --project tsconfig.build.json",
    "clean": "rm -rf ./lib",
    "copy-files": "cp ./package.json ./README.md ./LICENSE ./lib/",
    "build": "yarn clean && yarn build:ts && yarn copy-files",
    "pack": "yarn --cwd ./lib pack"
  },
  "exports": {
    "./*": "./filters/*.js",
    "./utils": "./utils.js"
  }
}
